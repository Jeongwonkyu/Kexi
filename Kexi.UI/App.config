  </startup>
  <appSettings>
    <!--<add key="FontFamily" value="Eras ITC" />-->
    <!--<add key="FontFamily" value="Consolas" />-->
    <add key="FontFamily" value="Microsoft Sans Serif" />
    <!--<add key="FontFamily" value="Century Gothic" />-->
    <!--<add key="FontFamily" value="Segoe UI" />-->
    <add key="FontSize" value="14" />
    <!-- whiteyBlue||obsi||obsiBlue||whiteyGreen||windows||obsiWindows|solarizedDark|obsiWindowsDark" -->
    <add key="Theme" value="obsiWindowsDark" />

    <add key="BeepOnNoMatch" value="false" />
    <add key="PopupTitleVisible" value="false" />
    <add key="PreferredEditorLocation" value="C:\Program Files (x86)\Vim\vim80\gvim.exe" />
    <add key="ProcessExplorerPath" value="C:\Program Files (x86)\SysInternals\Procexp.exe" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="GlobalHotKey" value="Oem3" /> <!-- Focus on Windows + GlobalHotKey -->
    <add key="ViewClipboardHotKey" value="Oem7" /> <!-- Focus on Windows + GlobalHotKey -->
    <add key="ShowHiddenItems" value="true" />
    <add key="DefaultViewMode" value="Detail" />
    <add key="ConsoleEncoding" value="850" />
    <add key="NotificationDuration" value="3" />
    <add key="LoadLastLayout" value="false" />
    <add key="Highlights" value="false" />
    <add key="CenterPopup" value="true" />
    <add key="PopupAnimation" value="Slide" />
    <add key="KeyboardMode" value="Undefined" />
    <add key="CornerRadius" value="4" />
  </appSettings>
